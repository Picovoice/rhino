{
  "name": "rhino-web-demo",
  "private": true,
  "version": "2.1.0",
  "description": "A basic demo to show how to use Rhino for web browsers, using the IIFE version of the library",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx pvbase64 -i ../../lib/common/rhino_params.pv -o ./rhino_params.js && cp ../../resources/contexts/wasm/clock_wasm.rhn ./",
    "start": "yarn run http-server -a localhost -p 5000"
  },
  "keywords": [
    "Picovoice",
    "Rhino",
    "browser",
    "voice ai",
    "speech recognition",
    "nlu",
    "private",
    "offline",
    "wasm",
    "webassembly"
  ],
  "author": "Picovoice Inc",
  "license": "Apache-2.0",
  "dependencies": {
    "@picovoice/rhino-web": "=2.1.0",
    "@picovoice/web-voice-processor": "=3.0.0"
  },
  "devDependencies": {
    "http-server": "^14.0.0"
  }
}
