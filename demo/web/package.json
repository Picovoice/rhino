{
  "name": "rhino-web-demo",
  "private": true,
  "version": "2.1.0",
  "description": "A basic demo to show how to use Rhino for web browsers, using the IIFE version of the library",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx pvbase64 -i ../../lib/common/rhino_params.pv -o ./rhino_params.js && ncp ../../resources/contexts/wasm/clock_wasm.rhn ./clock_wasm.rhn",
    "start": "http-server -a localhost -p 5000"
  },
  "keywords": [
    "Picovoice",
    "Rhino",
    "browser",
    "voice ai",
    "speech recognition",
    "nlu",
    "private",
    "offline",
    "wasm",
    "webassembly"
  ],
  "author": "Picovoice Inc",
  "license": "Apache-2.0",
  "dependencies": {
    "@picovoice/rhino-web": "^2.1.10",
    "@picovoice/web-voice-processor": "~4.0.2"
  },
  "devDependencies": {
    "http-server": "^14.0.0",
    "ncp": "^2.0.0"
  }
}
